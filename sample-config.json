{
  "templateDirectory": "../template-scala-console",
  "destinationDirectory": "../my-app",
  "directoryReplacements": {
    "/template/scala/console/": "/my-app/"
  },
  "textReplacements": {
    "template.scala.console": "my-app",
    "template-scala-console": "my-app",
    "template/scala/console": "my-app"
  },
  "ignoreDirectoryNamePatterns": [
    "projectFilesBackup",
    "target",
    "\\.git",
    "\\.idea"
  ],
  "ignoreFileNamePatterns": [
    "\\.gitattributes",
    ".*\\.iml"
  ]
}
